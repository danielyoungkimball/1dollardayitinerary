{
  "name": "1dollardayitinerary",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "postinstall": "yarn --cwd apps/web install",
    "vercel:install": "cd apps/web && yarn add --dev @types/react @types/react-dom"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "turbo": "^2.5.4",
    "typescript": "^5.3.3"
  }
}